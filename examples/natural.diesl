# Basic example using the macro-based natural syntax of DieSL.
# Demonstrates string operations.

change db.students:
    trim ending of firstName
    remove "rip", "aid" and "vit" from firstName

    replace "Isi" with "Daniel" in firstName

    replace in secondName:
        "Neil" with "Naile"
        "Deasmhumhain" with "Deasman"
        "flight" with "wood"
